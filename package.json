{
  "name": "my-webpack-typescript-library",
  "version": "1.0.2",
  "description": "Produce universal library with webpack and es6",
  "main": "lib/my-webpack-typescript-library.js",
  "config": {
    "cmd_webpack": "webpack --config .webpack/webpack.config.js"
  },
  "scripts": {
    "echovar": "cross-var echo $npm_package_config_cmd_webpack",
    "build": "%npm_package_config_cmd_webpack% --env prod",
    "dev": "%npm_package_config_cmd_webpack% --progress --colors --env dev",
    "dev:watch": "%npm_package_config_cmd_webpack% --progress --colors --env dev --watch",
    "test": "%npm_package_config_cmd_webpack% --progress --colors --env test",
    "test:watch": "%npm_package_config_cmd_webpack% --colors --env test -w --progress"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pmunin/webpack-typescript-library-starter.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/pmunin/webpack-typescript-library-starter",
  "dependencies": {
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.0",
    "@types/webpack": "^4.4.0",
    "@types/lodash": "^4.14.109",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "cross-var": "^1.1.0",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "jsdoc-babel": "^0.4.0",
    "mocha": "^5.2.0",
    "mocha-loader": "^1.1.3",
    "webpack": "^4.10.2",
    "webpack-cli": "^2.1.4",
    "yargs": "^11.0.0",
    "webpack-shell-plugin":"^0.5.0",
    "webpack-node-externals":"^1.7.2",
    "source-map-support":"^0.5.6",
    "typescript": "^2.9.1",
    "ts-loader": "^4.3.1",
    "declaration-bundler-webpack-plugin": "^1.0.3"
  }
}
